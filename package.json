{
  "name": "onchain-performance-profiler",
  "version": "1.0.0",
  "description": "On-chain Performance Profiler for multiple chains",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:collector\" \"npm run dev:api\" \"npm run dev:dashboard\"",
    "dev:collector": "tsx watch src/collector/index.ts",
    "dev:api": "tsx watch src/api/index.ts",
    "dev:dashboard": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "ethers": "^6.9.0",
    "@solana/web3.js": "^1.87.6",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "recharts": "^2.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}

